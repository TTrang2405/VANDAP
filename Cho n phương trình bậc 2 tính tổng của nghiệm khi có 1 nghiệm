n=int(input("Nhập số phương trình cần giải: "))
s=0
for i in range(1,n+1,1):
    a = float(input("Nhập a="))
    b = float(input("Nhập b="))
    c = float(input("Nhập c="))
    delta=b**2-4*a*c
    if a==0 and b!=0 :
        s+=(-c/b)
    elif delta==0:
       s+=(-b/(2*a))
print("Tổng nghiệm của phương trình 1 nghiệm là:", s)
