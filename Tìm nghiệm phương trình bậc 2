 import math
a = int(input("Nhập a: "))
b = int(input("Nhập b: "))
c = int(input("Nhập c: "))
delta = b*b - 4*a*c
if (delta>0):
    x1 = float((-b + math.sqrt(delta)) / (2 * a))
    x2 = float((-b - math.sqrt(delta)) / (2 * a))
    print(" Phương trình có 2 nghiệm phân biệt là:",x1,"và",x2)
if (delta==0):
    x1=x2= -b/(2*a)
    print(" Phương trình có 1 nghiệm duy nhất là:",x1,"và",x2)
if (delta<0):
    print(" Phương trình vô nghiệm")
